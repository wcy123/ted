<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">
    <include resource="org/springframework/boot/logging/logback/base.xml"/>
    <jmxConfigurator/>


    <logger name="org.springframework.web.servlet.mvc.method.annotation.ServletInvocableHandlerMethod"
            level="trace"/>
    <logger name="org.springframework.web.method.support.HandlerMethodArgumentResolverComposite"
            level="trace"/>
    <logger name="org.springframework.web.servlet.mvc.method.annotation.AbstractMessageConverterMethodProcessor"
            level="trace"/>
</configuration>
